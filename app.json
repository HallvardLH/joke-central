{
  "expo": {
    "name": "joke-central",
    "slug": "joke-central",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#6F53FD"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.whimsicalworks.jokecentral"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#6F53FD"
      },
      "package": "com.whimsicalworks.jokecentral"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-1354741235649835~2307627151",
          "iosAppId": "ca-app-pub-1354741235649835~7216338903"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "70759fa4-a8ea-487c-b2b2-0ed3073d440b"
      }
    }
  }
}